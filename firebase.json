{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "react-native": {
    "analytics_auto_collection_enabled": "false",
    "messaging_auto_init_enabled": "false",
    "messaging_ios_auto_register_for_remote_messages": "true"
  }
}
