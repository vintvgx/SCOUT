{
  "expo": {
    "name": "dpd.stats",
    "slug": "dpd_stats",
    "version": "1.1.8",
    "orientation": "portrait",
    "icon": "./assets/scout1024.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/scout_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vintvgx.scout",
      "userInterfaceStyle": "automatic"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vintvgx.scout"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "605ca6ac-8939-440c-baa7-0307f5f8d01d"
      }
    },
    "owner": "vintvgx",
    "plugins": [
      "expo-secure-store",
      [
      "@sentry/react-native/expo",
        {
          "organization": "communite",
          "project": "scout"
        }
      ],
      "expo-build-properties",
        {
          "ios": {
            "newArchEnabled": true
          },
          "android": {
            "newArchEnabled": true
          }
        }
    ]
    
  }
}
